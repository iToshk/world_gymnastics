= render partial: "shared/navbar_header"

.content.col-md-10
  h2
    a href="/code_of_points" ng-bind="code_of_points"
  p ng-bind="year_2013_to_2016"
  br
  /fa.fa.fa-spinner.fa-pulse
  .ul
    h3 ng-bind="rings"

    #p1.li
    br
      h4 ng-bind="rings_group_1"

      table.table.table-bordered.table-hover.table-condensed.table-striped
        thead
          tr
            th width="5%" ng-bind="number_of_skill"
            th width="4%" ng-bind="dificulty_of_skill"
            th width="75%" ng-bind="name_of_skill"
            th width="16%" ng-bind="sample_link"

        tbody
          .row.container.col-md-12
            tr ng-repeat="rings_group_1_skill in rings_group_1_skills"
              td width="5%" ng-bind="'I-' + rings_group_1_skill.number_of_skill"
              td width="4%" ng-bind="rings_group_1_skill.dificulty_of_skill"
              td width="75%" ng-bind="rings_group_1_skill.name_of_skill"
              td width="16%"
                a ng-href="{{rings_group_1_skill.sample_link}}" style="cursor: pointer" ng-hide="rings_group_1_skill.sample_link === ''"
                  | 1

